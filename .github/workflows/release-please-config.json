{
  "release-type": "node",
  "bump-minor-pre-major": true,
  "bump-patch-for-minor-pre-major": true,
  "changelog-sections": [
    {
      "type": "feat",
      "section": "Features"
    },
    {
      "type": "fix",
      "section": "Bug Fixes"
    },
    {
      "type": "deps",
      "section": "Dependencies"
    },
    {
      "type": "style",
      "section": "Style Changes"
    },
    {
      "type": "chore",
      "section": "Chores"
    },
    {
      "type": "build",
      "section": "Build System"
    },
    {
      "type": "cicd",
      "section": "CI/CD System"
    },
    {
      "type": "docs",
      "section": "Documentation"
    }
  ],
  "packages": {
    ".": {}
  },
  "extra-commit-mapping": {
    "\\[add\\]": "feat",
    "\\[add\\]!": "feat!",
    "\\[update\\]": "chore",
    "\\[update\\]!": "chore!",
    "\\[change\\]": "chore",
    "\\[change\\]!": "chore!",
    "\\[fix\\]": "fix",
    "\\[fix\\]!": "fix!",
    "\\[delete\\]": "feat!",
    "\\[delete\\]!": "feat!"
  }
}
